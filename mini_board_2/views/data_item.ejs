<% if (val != null){ %>
  <tr>
    <th style="width:100px;"><a class="home" href="/home/<%=val.attributes.user_id%>/1">
      <%= val.relations.user.attributes.name %></a></th>
    <td><%= val.attributes.message %></td>
    <%
    var d = new Date(val.attributes.create_at);
    var dstr = d.getFullYear()+'-'+(d.getMonth()+1)+'-'+d.getData()+
    ''+d.getHours()+':'+d.getMinutes()+':'+d.getSconds();
    %>
    <td style="width:150px;"><%=dstr%>></td>
  </tr>
<%}%>
